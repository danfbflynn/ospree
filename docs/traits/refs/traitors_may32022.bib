@article{Harrington2015,
abstract = {Many temperate and boreal tree species have a chilling requirement, that is, they need to experience cold temperatures during fall and winter to burst bud normally in the spring. Results from trials with 11 Pacific Northwest tree species are consistent with the concept that plants can accumulate both chilling and forcing units simultaneously during the dormant season and they exhibit a tradeoff between amount of forcing and chilling. That is, the parallel model of chilling and forcing was effective in predicting budburst and well chilled plants require less forcing for bud burst than plants which have received less chilling. Genotypes differed in the shape of the possibility line which describes the quantitative tradeoff between chilling and forcing units. Plants which have an obligate chilling requirement (Douglas-fir, western hemlock, western larch, pines, and true firs) and received no or very low levels of chilling did not burst bud normally even with long photoperiods. Pacific madrone and western redcedar benefited from chilling in terms of requiring less forcing to promote bud burst but many plants burst bud normally without chilling. Equations predicting budburst were developed for each species in our trials for a portion of western North America under current climatic conditions and for 2080. Mean winter temperature was predicted to increase 3.2–5.5°C and this change resulted in earlier predicted budburst for Douglas-fir throughout much of our study area (up to 74 days earlier) but later budburst in some southern portions of its current range (up to 48 days later) as insufficient chilling is predicted to occur. Other species all had earlier predicted dates of budburst by 2080 than currently. Recent warming trends have resulted in earlier budburst for some woody plant species; however, the substantial winter warming predicted by some climate models will reduce future chilling in some locations such that budburst will not consistently occur earlier.},
author = {Harrington, Constance A. and Gould, Peter J.},
doi = {10.3389/fpls.2015.00120},
file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Harrington{\&}Gould2015.pdf:pdf},
issn = {1664462X},
journal = {Frontiers in Plant Science},
keywords = {Budburst,Chilling,Dormancy,Forcing,Parallel model,Possibility line},
number = {MAR},
pages = {1--12},
title = {{Tradeoffs between chilling and forcing in satisfying dormancy requirements for Pacific Northwest tree species}},
volume = {6},
year = {2015}
}
@article{Heide1993,
abstract = {Dormancy release and thermal time to budburst as affected by duration of$\backslash$nchilling outdoors, followed by different flushing temperatures and$\backslash$ndaylengths in a phytotron, were studied in cuttings of several northern$\backslash$ntree species. In Betula pubescens, B. pendula and Prunus padus$\backslash$nvegetative buds were released from dormancy already in December, in$\backslash$nPopulus tremula in January, whereas in Alnus incana and A. glutinosa$\backslash$ndormancy was not released until February. Thermal time (day degrees$\backslash$n{\textgreater}0-degrees-C) to budburst decreased non-linearily with increasing$\backslash$nduration of chilling (i.e. duration outdoors), and the slope of this$\backslash$nrelationship differed among species. The estimated effective base$\backslash$ntemperature for accumulation of thermal time varied from +1-degrees-C in$\backslash$nP. tremula to -4-degrees-C in P. padus. The use of 0-degrees-C as base$\backslash$ntemperature is recommended. Long days reduced the thermal time to$\backslash$nbudburst at all flushing temperatures (9, 15 and 21-degrees-C) in all$\backslash$nthe above species and in Corylus avellana, whereas Sorbus aucuparia and$\backslash$nRubus idaeus showed no daylength response. Since the chilling$\backslash$nrequirement of all species was far exceeded even in a winter with$\backslash$nJanuary-March temperatures 6.5-degrees-C above normal, it is concluded$\backslash$nthat under Scandinavian conditions, the main effect of climatic warming$\backslash$nwould be earlier budburst and, associated with that, a longer growing$\backslash$nseason and increased risk of spring frost injury.},
author = {Heide, O. M.},
doi = {10.1034/j.1399-3054.1993.880401.x},
file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Heide/Physiologia Plantarum/Heide-1993-Physiologia{\_}Plantarum.pdf:pdf},
issn = {0031-9317},
journal = {Physiologia Plantarum},
keywords = {- bud dormancy,agricultural univ,as,chilling,climatic warming,daylength,dept of biology and,heide,m,n-1432,nature conservation,northern trees,norway,o,of,po box 5014,temperature,thermal time},
number = {4},
pages = {531--540},
title = {{Daylength and thermal time responses of budburst during dormancy release in some northern deciduous trees}},
volume = {88},
year = {1993}
}
@article{Huang2018c,
abstract = {Because perennial herbs of temperate climates develop their above-ground parts every year anew, their success critically depends on the timing and speed of this growth (growth phenology). These parameters can play a role in species coexistence and may differ along environmental gradients. Still, we know little about them, as most phenological data come from observations of flowering and to a lesser degree leafing onset. We collected data on growth phenology of about 400 perennial herbs in a botanical garden to make the results independent of local differences in climatic drivers as much as possible. Using these data, we determined species-specific parameters of Day of peak growth, Day of maturity and two types of growth rates associated with the change in plant size. Environmental conditions in which these species occur in the field were assessed using Ellenberg indicator values, which express species' optima along gradients of moisture, nutrients and temperature. Both timing and speed of growth estimated in the common garden were affected by light and moisture conditions of the habitats where the species typically occur. All parameters showed phylogenetic conservatism. We identified two relationships among these parameters of growth phenology: (1) species with early peak growth had high relative growth rates in contrast to late species; (2) tall species showed later peak growth than short species. The first relationship is associated with survival under forest canopy, where species are selected to grow early and fast before trees leaf out, which restricts their size. The latter is associated with (asymmetric) competition for light in open habitats, where the main selection factor is for tall stature, which cannot be attained early in the season. Synthesis. We show that large differences in size growth dynamics among herbaceous species are constrained by a few key trade-offs involving height at maturity, rate of growth and time when maximum height is attained. These trade-offs correspond to major selective forces acting on herbaceous plants in temperate climates.},
author = {Huang, Lin and Koubek, Tom{\'{a}}{\v{s}} and Weiser, Martin and Herben, Tom{\'{a}}{\v{s}}},
doi = {10.1111/1365-2745.12927},
file = {:Users/deirdreloughnan/Documents/Papers/bibfiles/Huang et al/Journal of Ecology/Huang{\_}et{\_}al-2018-Journal{\_}of{\_}Ecology.pdf:pdf},
issn = {13652745},
journal = {Journal of Ecology},
keywords = {Day of peak growth,Ellenberg values,flowering phenology,growth phenology,growth rate,light,nutrients,perennial plant,phylogenetic conservatism},
number = {4},
pages = {1621--1633},
title = {{Environmental drivers and phylogenetic constraints of growth phenologies across a large set of herbaceous species}},
volume = {106},
year = {2018}
}
